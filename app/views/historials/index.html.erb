<p id="notice"><%= notice %></p>

<h1>Historials</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historials.each do |histroal| %>
      <tr>
        <td><%= histroal.order_id %></td>
        <td><%= link_to 'Show', histroal %></td>
        <td><%= link_to 'Edit', edit_histroal_path(histroal) %></td>
        <td><%= link_to 'Destroy', histroal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Histroal', new_histroal_path %>
